<?xml version="1.0" ?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" updateCheck="false" xsi:noNamespaceSchemaLocation="ehcache.xsd" name="cacheManager">

  <diskStore path="java.io.tmpdir"/>
 
  <cache name="Employee"
    maxElementsInMemory="500"
    eternal="false"
    overflowToDisk="false"
    timeToLiveSeconds="0" />
    
  <cache name="Role"
    maxElementsInMemory="20"
    eternal="false"
    overflowToDisk="false"
    timeToLiveSeconds="0" />
  
  <cache name="Contact"
    maxElementsInMemory="500"
    eternal="false"
    overflowToDisk="false"
    timeToLiveSeconds="0" />

  <defaultCache maxElementsInMemory="10000"
    eternal="false"
    timeToLiveSeconds="86400"
    overflowToDisk="false"
    memoryStoreEvictionPolicy="LRU" />

</ehcache>
